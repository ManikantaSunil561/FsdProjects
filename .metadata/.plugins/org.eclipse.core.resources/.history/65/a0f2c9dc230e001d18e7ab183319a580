import java.util.Scanner;
public class ArrayAddition
{
	public static void main(String[] args)
	{
		int i,j,temp,r,c;
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter Row Size:");
		r=sc.nextInt();
		System.out.println("Enter Column Size:");
		c=sc.nextInt();
		int a[][]=new int[r][c];
		int b[][]=new int[r][c];
		int c[][]=new int[r][c];
	}
}
